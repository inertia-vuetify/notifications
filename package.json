{
    "name": "@gigerit/inertia-vuetify-notifications",
    "version": "1.0.2",
    "description": "Display Inertia.js flash messages using Vuetify snackbars",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "vite build --watch",
        "build": "vue-tsc --noEmit && vite build",
        "typecheck": "vue-tsc --noEmit"
    },
    "peerDependencies": {
        "@inertiajs/vue3": "^2.0.0",
        "vue": "^3.4.0",
        "vuetify": "^3.7.0"
    },
    "devDependencies": {
        "@inertiajs/vue3": "^2.0.0",
        "@types/node": "^25.0.3",
        "@vitejs/plugin-vue": "^6.0.3",
        "typescript": "^5.7.2",
        "vite": "^7.3.0",
        "vite-plugin-dts": "^4.3.0",
        "vue": "^3.5.13",
        "vue-tsc": "^3.1.8",
        "vuetify": "^3.7.5"
    },
    "keywords": [
        "vue",
        "vuetify",
        "inertia",
        "notifications",
        "snackbar",
        "flash-messages"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gigerIT/inertia-vuetify-notifications"
    }
}